%% ** (2) Figure 4, Triangles for cutting out watercolor (or other) paper

beginfig(4);

point r[];

w0 := w1;

%% *** (3) Line 1

% dotlabel.lft("$q_0$", q0);
% dotlabel.rt("$q_1$", q1);
% dotlabel.top("$q_2$", q2);

c0 := get_center w0;

%dotlabel.top("$c_0$", c0);

