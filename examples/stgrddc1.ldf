%dotlabel.lrt("$q_{18}$", q18);

q19 := q18 shifted (0, 1);

q20 := q16 rotated_around (q18, q19) 90;

%dotlabel.urt("$q_{20}$", q20);

bp := (q18 -- q20) intersection_point (q10 -- q14);

