w3 := w1;

string s;

j := 0;

for i = 0 upto 4:
   draw w2;
   draw w3;
   label(j, c2);
   j := j + 1;
   label(j, c3);
   j := j + 1;
   c2 *= c3 *= r1 *= r2 *= r3 *= r4 *= r5 *= r6 *= w2 *= w3 *= t1;
