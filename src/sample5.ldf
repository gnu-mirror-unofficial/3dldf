%% ** (2) Set variables

radius := 7cm;
do_black := false; %% true 

%% !! TODO:  LDF 2021.7.6.  Find out why using a newwrite_vector didn't work.

for i = 0 upto 7:
  s := "sample_5_" & decimal i & ".tex";
  nw[i] := s;
  s := "%%" & s & "\n%%";
  %message "s == " & s;
  write s to nw[i];
endfor;

input "astronmy.lmc";

