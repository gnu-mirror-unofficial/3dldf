Elliptical_Paraboloid::~Elliptical_Paraboloid(void)
{
   for (vector<Parabola*>::iterator iter = main_parabolae.begin();
        iter != main_parabolae.end();  
        ++iter)
       {
          delete *iter;
          *iter = 0;
       }

   main_parabolae.clear();    

   return;
}

