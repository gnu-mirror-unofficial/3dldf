@<Define |Pointer_Vector| functions@>=
template <class C, class D>
const Pointer_Vector<C, D>&
Pointer_Vector<C, D>::operator=(const vector<D>& v)
{

   clear();

   D* d;

   for(typename vector<C>::const_iterator iter = v.begin();
       iter != v.end();
       ++iter)
      {
         d = new D;

         *d = *iter;

         operator+=(d);         
      }

   return *this;
}

