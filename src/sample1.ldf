endfor;

draw_lattice k, .5 with_color red with_pen path_pen;
dotlabel.top("$L_0$", L0);

endfig with_focus f0;

%% **** (4) Figure 3 (focus 2)

beginfig(3);

draw c0 with_color blue with_pen path_pen;
