@<Define |Solid| functions@>=
Shape*
Solid::get_shape_ptr(const unsigned short curr_shape_type,
                     const unsigned short s) const
{
  bool DEBUG = false; /* |true|  */@;

  if (DEBUG)
    {
      cerr << "Entering Solid::get_shape_ptr().\n";
