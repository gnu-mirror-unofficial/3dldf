    {
      pts[19] = pts[20].mediate(pts[21], i /16.0);
      pts[19].drawdot(picture, 0, &q);
    }

  t.rotate(pts[4], pts[1]);
  t.rotate(pts[0], pts[2]);

  pts[20] = pts[17];
  pts[21] = pts[18];
  p[3] = p[0];

  pts[20] *= pts[21] *= p[3] *= t;
  
  p[3].draw(picture);
  for (i = 1; i < 16; ++i)
    {
