@q ****** (6).@> 

    shape_type = TRUNC_OCTAHEDRON_TYPE;
    surface_hiding_ctr = 0;
    decomposition_level = 0;

    do_output     = true;

    /*  !! TO DO.  TO DO:  Must calculate these!  */
    face_radius = 0;
