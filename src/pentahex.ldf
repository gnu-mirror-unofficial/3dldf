    dotlabel.lft("$p_{74}$", p74) on_picture v0;
    %label.top("$p_{30}$", p30);
  fi;

  q202 += ..;
  q202 += p73;
  
  forever:
    rotate_around p73 (p26, p28) -1;
    exitif xpart p73 <= xpart p74;
    q202 += p73;
  endfor;

  draw q202 on_picture v0;

