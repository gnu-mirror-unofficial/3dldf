    dotlabel.bot("$p_{13}$", p13);
    dotlabel.lft("$p_{14}$", p14);
    dotlabel.bot("$p_{15}$", p15);
  fi;

  p16 := mediate(p0, p1);

  p17 := mediate(p16, p12, .45);

  
  p18 := p17 rotated_around (p3, p12) 180;
  p19 := p17 rotated_around (p5, p12) 180;

