    dotlabel.top("$q_0$", p17);
    label.bot("$p_{17}$", p17);
    dotlabel.top("$q_1$", p18);
    label.bot("$p_{18}$", p18);
    dotlabel.top("$q_2$", p19);
    label.bot("$p_{19}$", p19);
  fi;

  q200 := q0 shifted (-4cm, 0, 10cm);
  p59 := p17 shifted (-4cm, 0, 10cm);
  
