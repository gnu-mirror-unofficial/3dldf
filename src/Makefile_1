#### $@:  Target
#### $<:  First prerequisite
#### $^:  The names of all the prerequisites, with spaces between them.
#### $*:  The stem with which an implicit rule matches

.PHONY: run

run: ttemp
	./ttemp

ttemp: ttemp.o
	g++ -o ttemp ttemp.o

ttemp.o: ttemp.cxx
	g++ -c -g -o ttemp.o ttemp.cxx
